{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "resolveJsonModule": true,
        "strict": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true
        },                         
        "exclude": ["node_modules"], 
        "include": ["src/**/*.ts"]
}